@model IEnumerable<Vidly.Models.Movie>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h1>Movies</h1>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Movie</th>
        </tr>
    </thead>

    @foreach (var mov in Model)
    {
        <tbody>
            <tr>
                <td>
                    @mov.Name
                </td>
            </tr>
        </tbody>

    }


</table>